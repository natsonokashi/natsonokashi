    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "animated-scrollto": "^1.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "clean-webpack-plugin": "^0.1.13",
    "express": "^4.14.0",
    "fake-style-loader": "^1.0.1",
    "fbemitter": "^2.1.1",
    "highlight.js": "^9.8.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "piping": "^1.0.0-rc.3",
    "qrcode.react": "^0.6.1",
    "react": "^15.4.0",
    "react-bootstrap": "^0.30.2",
    "react-document-meta": "^2.0.3",
    "react-dom": "^15.4.0",
    "react-router": "^2.6.1",
    "react-router-bootstrap": "^0.23.1",
    "superagent": "^3.0.0",
